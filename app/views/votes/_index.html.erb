<div class="container-fluid">
   <div class="row-fluid" >
    <div class="col-md-8 col-md-offset-2">
      <table class="table table-hover table-condensed table-bordered center">
				<% @votes.each do |v|%>
					<tr>
						<td>
						  <%= profile_pic_tag v.user.profile_picture.file.url, 'img-xs' %>
							<%= link_to v.user.user_name,user_path(v.user.id) %>
						</td>
					</tr>
				<% end %>
      </table>
    </div>
   </div>
</div>

