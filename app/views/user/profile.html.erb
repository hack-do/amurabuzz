<div class="container-fluid">
      <div class="row">
	      <div class="col-md-5  toppad  pull-right col-md-offset-3 ">
	       		<br>
	      </div>
	        
	      <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 col-xs-offset-0 col-sm-offset-0 col-md-offset-3 col-lg-offset-3 toppad" >
          <div class="panel panel-info">
            <div class="panel-heading">
              <h3 class="panel-title"><%= current_user.user_name%></h3>
            </div>
	          <div class="panel-body">
          		<div class="row-fluid">
		            <div class="col-md-3 col-lg-3 " align="center"> <%= image_tag current_user.avatar.url,size:"70x70"%> </div>
        			<div class=" col-md-9 col-lg-9 "> 
          				<table class="table table-user-information wrap_all">
		                    <tbody>
		                    
		                      <tr>
						        <th>Name</th>
						        <% if current_user.name%>
						        	<th><%= current_user.name%> </th>
						         <% end %>
						      </tr>
						    
						      
						      <tr>
						        <th>DOB</th>
						        <th><%= current_user.dob%> </th>
						      </tr>

						      <tr>
						        <th>Bio</th>
						        <th class="wrap"><%= current_user.bio %> </th>
						      </tr>

						      <tr>
						        <th>Email</th>
						        <th><%= current_user.email%> </th>
						      </tr>
	                 
		                    </tbody>
		                    <td><%= link_to 'Edit', edit_user_registration_path , class: "btn btn-success" %></td>
			            </table>
			       </div>
                </div>
              </div>
          </div>
        </div>
      </div>
    </div>




    